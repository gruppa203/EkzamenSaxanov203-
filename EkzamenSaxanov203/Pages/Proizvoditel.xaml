      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EkzamenSaxanov203.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Proizvoditel">

    <Grid>
        <DataGrid>
            <DataGrid.Columns>
                <DataGridTextColumn Header="Производитель" Binding="{Binding Naimenovanie_Proizvod}"/>

            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
